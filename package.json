{
  "name": "snack-store-back",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "ttsc -p tsconfig.build.json",
    "start": "node dist/server.js",
    "dev": "nodemon --watch \"src/\" --exec \"ts-node -r tsconfig-paths/register ./src/server.ts\" -e ts",
    "husky:prepare": "husky install",
    "lint:staged": "lint-staged",
    "test": "jest --passWithNoTests --runInBand --forceExit",
    "test:watch": "npm run test --watch",
    "test:staged": "npm run test --findRelatedTests",
    "test:coverage": "jest --passWithNoTests --runInBand --forceExit --coverage --collectCoverageFrom='./src/**'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.0.5",
    "tsconfig-paths": "^4.1.2",
    "ttypescript": "^1.5.15",
    "typescript": "^5.0.2",
    "typescript-transform-paths": "^3.4.6"
  }
}
